{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": { "maxLambdaSize": "50mb", "runtime": "nodejs18.x" }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "index.js" }
  ],
  "functions": {
    "index.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}